local kickTable = {}

local function addKickReasons(userIdReasons)
    for _, entry in ipairs(userIdReasons) do
        if type(entry) == "table" and entry.userId and entry.reason then
            kickTable[entry.userId] = entry.reason
        end
    end
end

local userIdReasons = {
